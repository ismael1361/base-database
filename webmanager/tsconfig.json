{
    "compilerOptions": {
        "target": "es2015",
        "lib": [
            "dom",
            "dom.iterable",
            "esnext"
        ],
        "allowJs": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "noFallthroughCasesInSwitch": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": false,
        "alwaysStrict": true,
        "noImplicitAny": false,
        "noEmit": true,
        "downlevelIteration": true,
        "jsx": "react-jsx",
        "rootDir": "./src",
        "baseUrl": "./src",
        "paths": {
            "src/*": [
                "./src/*"
            ],
        }
    },
    "include": [
        "./src/**/*",
    ],
    "exclude": [
        "node_modules"
    ]
}